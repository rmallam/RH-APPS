apiVersion: v1
kind: List
items:

- kind: StorageClass
  allowVolumeExpansion: true
  apiVersion: storage.k8s.io/v1
  metadata:
    name: azurefiles
  provisioner: kubernetes.io/azure-file
  mountOptions:
    - dir_mode=0777
    - file_mode=0777
    - mfsymlinks
    - cache=strict
    - actimeo=30
  parameters:
    skuName: Standard_LRS